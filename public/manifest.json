{
  "manifest_version": 3,
  "name": "Tabisphere",
  "version": "1.0.1",
  "description": "Organize webpages like a pro",
  "permissions": ["bookmarks", "contextMenus", "notifications", "tabs"],
  "action": {
    "default_title": "Tabisphere",
    "default_icon": {
      "128": "icon-128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "index.html",
  "icons": {
    "128": "icon-128.png"
  }
}
